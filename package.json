{
  "name": "cybdd",
  "version": "1.0.0",
  "description": "Optimum7 Test Automation",
  "main": "cypress.config.js",
  "scripts": {
    "test": "cypress run",
    "ui-regression": "cypress run --browser chrome --spec cypress/e2e/Login.feature/",
    "test:electron-pr": "cypress run --env tags=@sanity --browser electron --headless",
    "test:electron": "cypress run --browser electron --headless",
    "test:cypress-cloud": "cypress run --browser electron --headless --record --key c31b8723-9ae9-48d7-81ff-57385b9135ed",
    "test:chrome": "cypress run --browser chrome --headless",
    "test:firefox": "cypress run --browser firefox --headless",
    "test:edge": "cypress run --browser edge --headless",
    "test:parallel-pr": "cypress-parallel -s test:electron-pr -t 3 -d 'qa-scenarios' -m false",
    "test:spec": "npx cypress run --browser chrome --headless --spec '<specPath>'",
    "test:unit": "jest",
    "cypress": "cypress open",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open --env.dev",
    "start0": "webpack-dev-server --open --env.dev --host 0.0.0.0",
    "test:release": "webpack -p --env.test && webpack-dev-server --env.test",
    "release": "webpack -p --env.release && webpack-dev-server --open",
    "publish": "webpack -p --env.publish",
    "docs": "jsdoc -c jsdoc/jsdoc.conf.json jsdoc/README.md",
    "prepare": "husky install"
    
  },
  "keywords": [],
  "author": "Optimum7 LLC",
  "license": "ISC",
  "homepage":"https://www.optimum7.com",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.7",
    "@badeball/cypress-cucumber-preprocessor": "^20.0.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cdn-upload-webpack-plugin": "^0.2.3",
    "copy-webpack-plugin": "^6.0.0",
    "css-loader": "^5.2.7",
    "cy-mobile-commands": "^0.3.0",
    "cypress": "^13.8.1",
    "cypress-parallel": "^0.14.0",
    "cypress-real-events": "1.12.0",
    "cypress-recurse": "1.35.3",
    "cypress-wait-until": "3.0.1",
    "docdash": "^1.2.0",
    "es6-promise": "^4.2.8",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "joi": "^17.11.0",
    "jsdoc": "^3.6.3",
    "jsdoc-export-default-interop": "^0.3.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^1.6.2",
    "prettier": "^2.7.1",
    "remove-files-webpack-plugin": "^1.4.0",
    "sass": "^1.75.0",
    "sass-loader": "^10.3.1",
    "style-loader": "^1.1.2",
    "transform-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webworkify-webpack": "^2.1.5"
  },

  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@splidejs/splide": "^4.1.4",
    "@turf/along": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/buffer": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "ajv": "^6.12.6",
    "body-scroll-lock": "^4.0.0-beta.0",
    "jquery": "^3.6.0",
    "lodash.capitalize": "^4.2.1",
    "maplibre-gl": "2.4.0",
    "select2": "^4.0.13",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,html,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,html,css,json}": "prettier --write"
  },
 
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "./cypress/e2e/**/*.js"
    ],
    "omitFiltered": true,
    "html": {
      "enabled": true,
      "output": "cypress/results/cucumber-report.html"
    },
    "json": {
      "enabled": true,
      "output": "cypress/results/cucumber-report.json"
    }
  }
}